{
  "name": "fastapi-todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm next-dev\" \"pnpm fast-dev\"",
    "fast-dev": "pip install -r requirements.txt && uvicorn --app-dir api index:app --reload",
